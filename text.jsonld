{
  "@context": {
    "txt": "https://humanities.knaw.nl/def/text/",
    "dc": "http://purl.org/dc/elements/1.1/",
    "dcterms": "http://purl.org/dc/terms/",
    "oa": "http://www.w3.org/ns/oa#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "pagexml": "http://schema.primaresearch.org/PAGE/gts/pagecontent/2013-07-15/",
    "tei": "http://www.tei-c.org/ns/1.0#",
    "folia": "https://w3id.org/folia/v2/",
    "iiif_prezi": "http://iiif.io/api/presentation/3#",
    "Token" : {
        "@id": "txt:Token",
        "@type": "@id",
        "rdfs:label": "Token",
        "rdfs:comment": "A small unit of text when text is separated on whitespace, punctuation and numerals. Not necessarily carrying much meaning by itself. Not by definition equal to a word."
    },
    "Word" : {
        "@id": "txt:Word",
        "@type": "@id",
        "rdfs:label": "Word",
        "rdfs:comment": "A fundamental grammatical meaning-carrying item in text structure.",
        "owl:sameAs": [ "folia:Word", "tei:w" ]
    },
    "Line" : {
        "@id": "txt:Line",
        "@type": "@id",
        "rdfs:label": "Line",
        "rdfs:comment": "A line in text structure. A line is physically stretches accross one specific spatial dimension (the presentational orientation is not specified nor implied). A line itself by definitions contains no linebreaks/newlines (including leading/trailing newlines).",
        "owl:sameAs": [ "pagexml:TextLine", "tei:Line" ]
    },
    "Sentence" : {
        "@id": "txt:Sentence",
        "@type": "@id",
        "rdfs:label": "Sentence",
        "rdfs:comment": "A grammatically complete syntactic unit, consisting of words.",
        "owl:sameAs": [ "folia:Sentence", "tei:s" ]
    },
    "Paragraph" : {
        "@id": "txt:Paragraph",
        "@type": "@id",
        "rdfs:label": "Paragraph",
        "rdfs:comment": "A self-contained unit of discourse. Leading/trailing whitespace or newlines are not included.",
        "owl:sameAs": [ "tei:p", "folia:Paragraph" ]
    },
    "Span" : {
        "@id": "txt:Span",
        "@type": "@id",
        "rdfs:label": "Span",
        "rdfs:comment": "An arbitrary generic grouping of a relatively small amount of text.",
        "owl:sameAs": [ "tei:span", "folia:String" ]
    },
    "Quote" : {
        "@id": "txt:quote",
        "@type": "@id",
        "rdfs:label": "Quote",
        "rdfs:comment": "Text representing a quote or citation",
        "owl:sameAs": [ "tei:q", "folia:Quote" ]
    },
    "Division" : {
        "@id": "txt:Division",
        "@type": "@id",
        "rdfs:label": "Division",
        "rdfs:comment": "An arbitrary generic grouping of a larger amount of text in discourse.",
        "owl:sameAs": [ "pagexml:TextRegion", "tei:div", "folia:Division" ]
    },
    "Section" : {
        "@id": "txt:Section",
        "@type": "@id",
        "rdfs:label": "Section",
        "rdfs:comment": "A grouping of a larger amount of text in discourse. This is more specific than 'Division'. Sections sometimes carry numbering."
    },
    "Page": {
        "@id": "txt:Page",
        "@type": "@id",
        "rdfs:label": "Page",
        "rdfs:comment": "A large span of text on the same unit of a certain physical medium.",
        "owl:sameAs": "pagexml:Page"
    },
    "Letter": {
        "@id": "txt:Letter",
        "@type": "@id",
        "rdfs:label": "Letter",
        "rdfs:comment": "A body of text that represents written correspondence/communication between people."
    },
    "Linebreak" : {
        "@id": "txt:Linebreak",
        "@type": "@id",
        "rdfs:label": "Linebreak",
        "rdfs:comment": "Point at which one line ends and a new line is formed. This annotation is only used in contexts where line breaks are not already implied and should be used sparingly.  The so-called carriage-return is already implied and not encoded separately.",
        "owl:sameAs": [ "tei:lb", "folia:Linebreak" ]
    },
    "Head": {
        "@id": "txt:Head",
        "@type": "@id",
        "rdfs:label": "Head",
        "rdfs:comment": "Titles of sections and subsections",
        "owl:sameAs": ["tei:head", "folia:head" ]
    },
    "List": {
        "@id": "txt:List",
        "@type": "@id",
        "rdfs:label": "List",
        "rdfs:comment": "A text portion that contains an enumerisation of itemisation of different items, often separated demarked by a bullet, hyphen or numeral.",
        "owl:sameAs": [ "tei:list", "folia:List" ]
    },
    "ListItemMarker": {
        "@id": "txt:ListItemMarker",
        "@type": "@id",
        "rdfs:label": "ListItemMarker",
        "rdfs:comment": "The numeral/bullet/hyphen that marks items in a list."
    },
    "ListItem": {
        "@id": "txt:ListItem",
        "@type": "@id",
        "rdfs:label": "ListItem",
        "rdfs:comment": "An item in a list. Does not include the numeral/bullet/hyphen.",
        "owl:sameAs": ["tei:item", "folia:ListItem" ]
    },
    "Whitespace" : {
        "@id": "txt:Whitespace",
        "@type": "@id",
        "rdfs:label": "Whitespace",
        "rdfs:comment": "Empty parts of the text, i.e. gaps in text, may be either horizontal or vertical.",
        "owl:sameAs": ["tei:space", "folia:Whitespace" ]
    },
    "Figure" : {
        "@id": "txt:Figure",
        "@type": "@id",
        "rdfs:label": "Figure",
        "rdfs:comment": "A part of the document that contains one or more images, may be accompanied by a caption.",
        "owl:sameAs": ["tei:figure", "folia:Figure" ]
    },
    "Image" : {
        "@id": "txt:Image",
        "@type": "@id",
        "rdfs:label": "Image",
        "rdfs:comment": "Part of the document where an image is located. Non-textual.",
        "owl:sameAs": "tei:graphic"
    },
    "Caption" : {
        "@id": "txt:Caption",
        "@type": "@id",
        "rdfs:label": "Caption",
        "rdfs:comment": "Text that constitutes the caption for a figure or table",
        "owl:sameAs": ["tei:caption", "folia:Caption"]
    },
    "Table" : {
        "@id": "txt:Table",
        "@type": "@id",
        "rdfs:label": "Table",
        "rdfs:comment": "Text that constitutes a table",
        "owl:sameAs": ["tei:table", "folia:Table"]
    },
    "Row" : {
        "@id": "txt:Row",
        "@type": "@id",
        "rdfs:label": "Row",
        "rdfs:comment": "Text that constitutes a row in a table",
        "owl:sameAs": ["tei:row", "folia:Row"]
    },
    "Cell" : {
        "@id": "txt:Cell",
        "@type": "@id",
        "rdfs:label": "Cell",
        "rdfs:comment": "Text that constitutes a single cell in a table",
        "owl:sameAs": ["tei:cell", "folia:Cell"]
    },
    "Column" : {
        "@id": "txt:Column",
        "@type": "@id",
        "rdfs:label": "Column",
        "rdfs:comment": "Text that corresponds to a column in a multi-columned layout. Not used in tables."
    },
    "Note" : {
        "@id": "txt:Note",
        "@type": "@id",
        "rdfs:label": "Note",
        "rdfs:comment": "A footnote, endnote or note in the marginalia. This contains the actual note.",
        "owl:sameAs": ["tei:note", "folia:Note"]
    },
    "Reference" : {
        "@id": "txt:Reference",
        "@type": "@id",
        "rdfs:label": "Reference",
        "rdfs:comment": "A reference to another location in the text, such as to a footnote/endnote",
        "owl:sameAs": ["tei:ref", "folia:Reference"]
    },
    "Entity" : {
        "@id": "txt:Entity",
        "@type": "@id",
        "rdfs:label": "Entity",
        "rdfs:comment": "A span of text that refers to some kind of named entity",
        "owl:sameAs": ["tei:rs", "tei:name", "folia:Entity"]
    },
    "Highlight" : {
        "@id": "txt:Highlight",
        "@type": "@id",
        "rdfs:label": "Highlight",
        "rdfs:comment": "A highlighted/emphasised piece of text, in some presentational form.",
        "owl:sameAs": ["tei:hi", "folia:TextMarkupStyle"]
    },
    "Correction" : {
        "@id": "txt:Correction",
        "@type": "@id",
        "rdfs:label": "Correction",
        "rdfs:comment": "A correction on a text",
        "owl:sameAs": ["tei:corr", "folia:Correction"]
    },
    "Canvas": {
      "@id": "iiif_prezi:Canvas",
      "@context": {
        "partOf": {
          "@id": "dcterms:partOf",
          "@type": "@id",
          "@container": "@set"
        }
      }
    },
    "n": {
        "@id": "txt:n",
        "@type": "@id",
        "rdfs:label": "number",
        "rdfs:comment": "Sequence number",
        "owl:sameAs": ["tei:n", "folia:NAttribute"]
    }
  }
}
